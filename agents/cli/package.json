{
  "name": "@genesis/cli",
  "version": "1.0.0",
  "description": "Genesis CLI - Command-line interface for Genesis Agent SDK",
  "type": "module",
  "main": "dist/genesis-cli.js",
  "bin": {
    "genesis": "./dist/genesis-cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "link": "npm run build && npm link",
    "unlink": "npm unlink -g"
  },
  "keywords": [
    "genesis",
    "cli",
    "scaffolding",
    "next.js"
  ],
  "author": "Genesis Agent SDK",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "ora": "^8.0.1",
    "chalk": "^5.3.0",
    "axios": "^1.6.7"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/inquirer": "^9.0.7",
    "typescript": "^5.3.3"
  }
}
